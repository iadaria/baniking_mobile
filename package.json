{
  "name": "baniking_mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android-shake": "adb shell input keyevent 82",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build-android": "cd android && ./gradlew assembleRelease && cd ../",
    "debug-android": "cd android && ./gradlew assembleDebug && cd ../",
    "ios-se": "react-native run-ios --simulator=\"iPhone 6\"",
    "clean": "watchman watch-del-all && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-map-* && yarn start --reset-cache",
    "reset-cache": "yarn cache clean --force && react-native start --reset-cache",
    "android-clean": "cd android && ./gradlew cleanBuildCache",
    "ios-reset-cache": "pod cache clean --all && pod deintergrate && pod clean && xcodebuild clean",
    "clean-build": "rm -rf ios/build android/app/build",
    "reset": "react-native run-android -- --reset-cache",
    "eject": "react-native eject",
    "clean-project": "npx react-native-clean-project",
    "pod-update": "yarn upgrade --latest && npx pod-install",
    "pod": "npx pod-install",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "other-clean": "rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-* && watchman watch-del-all",
    "remove-android-build": "rm -rf android/build",
    "android-build": "cd android && ./gradlew build && cd ..",
    "brew": "brew update && brew upgrade",
    "android-bundle-release": "cd android && ./gradlew bundleRelease && cd ..",
    "android-unistall": "adb uninstall \"com.baniking_mobile\"",
    "android-release": "npx react-native run-android --variant=release"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
